#!/bin/sh
CONFIG_LIBS="-LIBPATH:D:\OSG\install_gdal/lib gdal"
CONFIG_DEP_LIBS="wbemuuid ws2_32 D:/OSG/sqlite/sqlite3.lib "C:/Program Files/MySQL/MySQL Server 8.0/lib/mysqlclient.lib" "C:/Program Files/Microsoft SQL Server/Client SDK/ODBC/170/SDK/Lib/x64/msodbcsql17.lib" legacy_stdio_definitions.lib odbccp32.lib D:/OSG/install_curl/lib/libcurl_imp.lib psapi D:/OSG/install_proj/lib/proj.lib"
CONFIG_PREFIX="D:\OSG\install_gdal"
CONFIG_CFLAGS="-ID:\OSG\install_gdal/include"
CONFIG_DATA="D:\OSG\install_gdal/share/gdal"
CONFIG_VERSION="3.8.5"
CONFIG_GNM_ENABLED=yes
CONFIG_FORMATS="JPEG raw GTIFF MEM vrt Derived HFA SDTS NITF GXF AAIGrid CEOS SAR_CEOS XPM DTED JDEM Envisat ELAS FIT L1B RS2 ILWIS RMF Leveller SGI SRTMHGT IDRISI GSG ERS PALSARJaxa DIMAP GFF COSAR PDS ADRG COASP TSX Terragen BLX MSGN TIL R northwood SAGA XYZ ESRIC HF2 KMLSUPEROVERLAY CTG ZMap NGSGEOID IRIS MAP CALS SAFE SENTINEL2 PRF MRF WMTS GRIB BMP TGA STACTA BSB AIGrid ARG USGSDEM AirSAR OZI PCIDSK SIGDEM RIK STACIT PDF PNG GIF WCS HTTP Zarr DAAS EEDA PLMOSAIC WMS OGCAPI Rasterlite PCRaster MEM geojson TAB Shape KML VRT AVC GML CSV DGN GMT NTF S57 Tiger Geoconcept GeoRSS DXF PGDump GPSBabel EDIGEO SXF OpenFileGDB WAsP Selafin JML VDV FlatGeobuf MapML JSONFG SDTS CSW PLSCENES WFS NGW Elastic Idrisi PDS SQLite GeoPackage OSM VFK AmigoCloud Carto MySQL MSSQLSpatial ODBC PGeo CAD GTFS"
CONFIG_PLUGINDIR="D:\OSG\install_gdal/lib/gdalplugins"
usage()

{
	cat <<EOF
Usage: gdal-config [OPTIONS]
Options:
	[--prefix[=DIR]]
	[--libs]
	[--dep-libs]
	[--cflags]
	[--datadir]
	[--version]
	[--ogr-enabled]
	[--gnm-enabled]
	[--formats]
	[--plugindir]
EOF
	exit $1
}

if test $# -eq 0; then
	usage 1 1>&2
fi

case $1 in
  --libs)
    echo $CONFIG_LIBS
    ;;

  --dep-libs)
    echo $CONFIG_DEP_LIBS
    ;;

  --cflags)
    echo $CONFIG_CFLAGS
    ;;

  --datadir)
    echo $CONFIG_DATA
    ;;

  --prefix)
    echo $CONFIG_PREFIX
    ;;

  --version)
    echo $CONFIG_VERSION
    ;;

  --ogr-enabled)
    echo yes
    ;;

  --gnm-enabled)
    echo $CONFIG_GNM_ENABLED
    ;;

  --formats)
    echo $CONFIG_FORMATS
    ;;

  --plugindir)
    echo $CONFIG_PLUGINDIR
    ;;

  *)
    usage 1 1>&2
    ;;

esac
